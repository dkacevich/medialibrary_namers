{
    "name": "dkacevich/medialibrary-namers",
    "description": "Custom path and file namers for spatie/laravel-medialibrary based on model and collection name",
    "keywords": [
        "dkacevich",
        "medialibrary-namers"
    ],
    "homepage": "https://github.com/dkacevich/medialibrary-namers",
    "license": "MIT",
    "authors": [
        {
            "name": "Dima Kacevich",
            "email": "d.katsevich.4@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-medialibrary": "^11.0.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Dkacevich\\MedialibraryNamers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dkacevich\\MedialibraryNamers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
